{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "country": {
      "type": "string",
      "minLength": 2
    },
    "city": {
      "type": "string",
      "minLength": 2
    },
    "street_address": {
      "type": "string",
      "minLength": 3
    },
    "file_name":  {
      "type": "string",
      "minLength": 5
    },
    "id_house": {
      "type": "integer",
      "minimum": 0
    }
  },
  "required": [ "street_address", "city", "country", "file_name" ],
  "additionalProperties": true
}


